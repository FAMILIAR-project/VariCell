import: 

"Ressources/usesCases/wikiMatrixDSL/CSV/Usability.csv" as usability
"Ressources/usesCases/wikiMatrixDSL/CSV/Datastorage.csv" as datastorage
"Ressources/usesCases/wikiMatrixDSL/CSV/SpecialFeatures.csv" as special
"Ressources/usesCases/wikiMatrixDSL/CSV/SyntaxFeatures.csv" as syntax
"Ressources/usesCases/wikiMatrixDSL/CSV/Extras.csv" as extra
"Ressources/usesCases/wikiMatrixDSL/CSV/Statistics.csv" as statistics
"Ressources/usesCases/wikiMatrixDSL/CSV/HostingFeatures.csv" as hosting
"Ressources/usesCases/wikiMatrixDSL/CSV/Output.csv" as output
"Ressources/usesCases/wikiMatrixDSL/CSV/GeneralFeatures.csv" as generalFts
"Ressources/usesCases/wikiMatrixDSL/CSV/CommonFeatures.csv" as common
"Ressources/usesCases/wikiMatrixDSL/CSV/Links.csv" as links

name: "WikiMatrix"


default: 
	parsing:
	 	key: "Identifior"
	only_products: "Groupswiki" "Socialtext" "Sputnik" "OpenWikiNG" "TracWiki" "MidgardWiki" "Hatta" "UseMod" "SubWiki" "Nexdo" "Intodit" "MoniWiki" "PBwiki" "XoWiki" "JaWiki" "PukiWiki" "Wiki" "WikiSpot" "Bitweaver" "Gazest" "Gitit" "SamePage" "EditMe" "ThoughtFarmer" "WikkaWiki" 

view: usability
only_features: "SectionEditing" "PageTemplates" "DoubleClickEdit" "Toolbar" "WYSIWYGEditing" 

view: datastorage
only_features: "TextFiles" "MySQL" "PostgreSQL" "Oracle" "SQLite" 

view: special
only_features: "UnicodeSupport" "RighttoLeftSupport" "InterfaceLanguages" "Emailnotification" "Comments" 

view: syntax
only_features: "HTMLTags" "Mathformulas" "Tables" "CREOLEsupport" "MarkdownSupport" 

view: extra
only_features: "Calendar" "ImageGalleries" "Forums" "Blogs" "TicketSystem" 

view: statistics
only_features: "RecentChanges" "WantedPages" "OrphanedPages" "Most_LeastPopular" "RecentVisitors" 

view: hosting
only_features: "StorageQuota" "BandwidthQuota" "OtherLimits" "TopicRestrictions" "CorporateBranding" 

view: output
only_features: "HTML" "CSSStylesheets" "PrinterFriendly" "MobileFriendly" "ThemesSkins" 

view: generalFts
only_features: "Version" "LastReleaseDate" "Author" "URL" "FreeandOpenSource" 

view: common
only_features: "Preview" "MinorChanges" "ChangeSummary" "PageHistory" "PageRevisions" 

view: links
only_features: "CamelCase" "Freelinks" "Backlinks" "InterWiki" "SisterWiki" 




