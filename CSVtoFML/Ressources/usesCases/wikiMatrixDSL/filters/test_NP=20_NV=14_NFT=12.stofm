import: 

"Ressources/usesCases/wikiMatrixDSL/CSV/Usability.csv" as usability
"Ressources/usesCases/wikiMatrixDSL/CSV/Output.csv" as output
"Ressources/usesCases/wikiMatrixDSL/CSV/GeneralFeatures.csv" as generalFts
"Ressources/usesCases/wikiMatrixDSL/CSV/CommonFeatures.csv" as common
"Ressources/usesCases/wikiMatrixDSL/CSV/Links.csv" as links
"Ressources/usesCases/wikiMatrixDSL/CSV/Datastorage.csv" as datastorage
"Ressources/usesCases/wikiMatrixDSL/CSV/SpecialFeatures.csv" as special
"Ressources/usesCases/wikiMatrixDSL/CSV/SyntaxFeatures.csv" as syntax
"Ressources/usesCases/wikiMatrixDSL/CSV/Extras.csv" as extra
"Ressources/usesCases/wikiMatrixDSL/CSV/HostingFeatures.csv" as hosting
"Ressources/usesCases/wikiMatrixDSL/CSV/Statistics.csv" as statistics
"Ressources/usesCases/wikiMatrixDSL/CSV/Functionalities.csv" as functionalities
"Ressources/usesCases/wikiMatrixDSL/CSV/MediaandFiles.csv" as media
"Ressources/usesCases/wikiMatrixDSL/CSV/SystemRequirements.csv" as sysrequirements

name: "WikiMatrix"


default: 
	parsing:
	 	key: "Identifior"
	only_products: "PBwiki" "DokuWiki" "MediaWiki" "PmWiki" "DrupalWiki" "TWiki" "MoinMoin" "Confluence" 

view: usability
only_features: "SectionEditing" "PageTemplates" "DoubleClickEdit" "Toolbar" "WYSIWYGEditing" "AccessKeys" "AutoSignature" 

view: output
only_features: "HTML" "CSSStylesheets" "PrinterFriendly" "MobileFriendly" "ThemesSkins" "RSSFeeds" "ATOMFeeds" "Abbreviations" "AutoTOC" "RawExport" "HTMLExport" "XMLexport" 

view: generalFts
only_features: "Version" "LastReleaseDate" "Author" "URL" "FreeandOpenSource" "License" "ProgrammingLanguage" "DataStorage" "LicenseCost_Fee" "Developmentstatus" "IntendedAudience" 

view: common
only_features: "Preview" "MinorChanges" "ChangeSummary" "PageHistory" "PageRevisions" "RevisionDiffs" "PageIndex" "PluginSystem" 

view: links
only_features: "CamelCase" "Freelinks" "Backlinks" "InterWiki" "SisterWiki" "ImageLinks" "WindowsShares" "PageRedirects" 

view: datastorage
only_features: "TextFiles" "MySQL" "PostgreSQL" "Oracle" "SQLite" "BerkeleyDB" "RCS" "Other" 

view: special
only_features: "UnicodeSupport" "RighttoLeftSupport" "InterfaceLanguages" "Emailnotification" "Comments" "Categories" "Namespaces" "PageRedirection" "ConflictHandling" "Search" "WikiFarming" "StructuredData" 

view: syntax
only_features: "HTMLTags" "Mathformulas" "Tables" "CREOLEsupport" "MarkdownSupport" "TextileSupport" "BBCodeSupport" "EmoticonImages" "SyntaxHighlighting" "Footnotes" "Quoting" "InternalComments" 

view: extra
only_features: "Calendar" "ImageGalleries" "Forums" "Blogs" "TicketSystem" 

view: hosting
only_features: "StorageQuota" "BandwidthQuota" "OtherLimits" "TopicRestrictions" "CorporateBranding" "OwnDomain" "Adsallowed" 

view: statistics
only_features: "RecentChanges" "WantedPages" "OrphanedPages" "Most_LeastPopular" "RecentVisitors" "Analysis" 

view: functionalities
only_features: "UnicodeSupport" "RSSFeeds" "EmbeddedFlash" "ImageEditing" 

view: media
only_features: "FileAttachments" "MediaRevisions" "EmbeddedFlash" "EmbeddedVideo" "ImageEditing" "SVGEditing" "MindMapEditing" "MediaSearch" 

view: sysrequirements
only_features: "OperatingSystem" "RootAccess" "Webserver" "OtherRequirements" 



