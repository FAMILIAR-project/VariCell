import: 

"Ressources/usesCases/wikiMatrixDSL/CSV/Usability.csv" as usability
"Ressources/usesCases/wikiMatrixDSL/CSV/Datastorage.csv" as datastorage
"Ressources/usesCases/wikiMatrixDSL/CSV/Output.csv" as output
"Ressources/usesCases/wikiMatrixDSL/CSV/GeneralFeatures.csv" as generalFts
"Ressources/usesCases/wikiMatrixDSL/CSV/CommonFeatures.csv" as common
"Ressources/usesCases/wikiMatrixDSL/CSV/Links.csv" as links

name: "WikiMatrix"


default: 
	parsing:
	 	key: "Identifior"
	only_products: "Groupswiki" "Socialtext" "Sputnik" "OpenWikiNG" "TracWiki" "MidgardWiki" "Hatta" "UseMod" "SubWiki" "Nexdo" "Intodit" "MoniWiki" "PBwiki" "XoWiki" "JaWiki" "PukiWiki" "Wiki" "WikiSpot" "Bitweaver" "Gazest" "Gitit" "SamePage" "EditMe" "ThoughtFarmer" "WikkaWiki" "WikionaStick" "WxWikiServer" "MindTouch" "DokuWiki" "BusinessWiki" 

view: usability
only_features: "SectionEditing" "PageTemplates" "DoubleClickEdit" "Toolbar" "WYSIWYGEditing" 

view: datastorage
only_features: "TextFiles" "MySQL" "PostgreSQL" "Oracle" "SQLite" 

view: output
only_features: "HTML" "CSSStylesheets" "PrinterFriendly" "MobileFriendly" "ThemesSkins" 

view: generalFts
only_features: "Version" "LastReleaseDate" "Author" "URL" "FreeandOpenSource" 

view: common
only_features: "Preview" "MinorChanges" "ChangeSummary" "PageHistory" "PageRevisions" 

view: links
only_features: "CamelCase" "Freelinks" "Backlinks" "InterWiki" "SisterWiki" 



