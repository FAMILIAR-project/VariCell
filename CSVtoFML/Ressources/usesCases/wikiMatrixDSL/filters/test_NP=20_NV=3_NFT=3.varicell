import: 

"Ressources/usesCases/wikiMatrixDSL/CSV/Usability.csv" as usability
"Ressources/usesCases/wikiMatrixDSL/CSV/Output.csv" as output
"Ressources/usesCases/wikiMatrixDSL/CSV/GeneralFeatures.csv" as generalFts

name: "WikiMatrix"


default: 
	parsing:
	 	key: "Identifior"
	only_products: "Groupswiki" "Socialtext" "Sputnik" "OpenWikiNG" "TracWiki" "MidgardWiki" "Hatta" "UseMod" "SubWiki" "Nexdo" "Intodit" "MoniWiki" "PBwiki" "XoWiki" "JaWiki" "PukiWiki" "Wiki" "WikiSpot" "Bitweaver" "Gazest" 

view: usability
only_features: "SectionEditing" "PageTemplates" "DoubleClickEdit" 

view: output
only_features: "HTML" "CSSStylesheets" "PrinterFriendly" 

view: generalFts
only_features: "Version" "LastReleaseDate" "Author" 




